        "settings": {
          "fontStyle": "normal",
          "foreground": "#91B859"
        }
      },
      {
        "name": "Class, Support",
        "scope": [
          "entity.name.class",
          "entity.name.type.class",
          "support.type",
          "support.class",
          "support.orther.namespace.use.php",
          "meta.use.php",
          "support.other.namespace.php",
